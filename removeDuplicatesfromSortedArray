class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int index=0;
        if (nums.size()==0) return 0;
        for(i=0;i<nums.size();i++){
            if(nums[index]!=nums[i]{
               index++;
               nums[index]=nums[i];
            }
              
        
    }
    return index+1;
               }
};
