#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

void BubbleSort(int A[],int n)
{
    for(int k=0;k<n-1;k++){
        //int flag=0;
        for(int i=0;i<n-k-1;i++){
            if(A[i]>A[i+1]){
            int temp=A[i];
            A[i]=A[i+1];
            A[i+1]=temp;
            }
               
        }
      // if(flag==0) break; 
}
}
int main(){
    int A[]={2,7,4,1,5,3};
    BubbleSort(A,6);
    for(int i=0;i<6;i++)
        cout<<A[i]<<" ";
}
